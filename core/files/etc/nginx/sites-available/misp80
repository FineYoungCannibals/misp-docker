server {
    listen 80 default_server;
    listen [::]:80 default_server;

    access_log off;
    log_not_found off;
    error_log  /dev/stderr error;

    add_header X-Frame-Options SAMEORIGIN;

    # Trust proxy headers from Traefik
    real_ip_header X-Forwarded-For;
    set_real_ip_from 172.16.0.0/12;  # Docker network range
    set_real_ip_from 10.0.0.0/8;    # Docker network range
    set_real_ip_from 192.168.0.0/16; # Docker network range
    real_ip_header X-Forwarded-For;    
    real_ip_recursive on;    

    include includes/misp;
}
